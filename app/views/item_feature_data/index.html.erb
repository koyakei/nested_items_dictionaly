<p id="notice"><%= notice %></p>

<h1>Item Feature Data</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Number</th>
      <th>Feature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @item_feature_data.each do |item_feature_datum| %>
      <tr>
        <td><%= item_feature_datum.item %></td>
        <td><%= item_feature_datum.number %></td>
        <td><%= item_feature_datum.feature %></td>
        <td><%= link_to 'Show', item_feature_datum %></td>
        <td><%= link_to 'Edit', edit_item_feature_datum_path(item_feature_datum) %></td>
        <td><%= link_to 'Destroy', item_feature_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item Feature Datum', new_item_feature_datum_path %>
